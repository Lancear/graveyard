{
  "tasks": {
    "start": "deno run --env-file main.ts",
    "dev": "deno run --watch --env-file main.ts",
    "test": "deno test --no-check",
    "check": "deno check main.ts",
    "fmt": "deno fmt",
    "lint": "deno run --allow-env --allow-read --allow-run npm:oxlint@latest"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  }
}
